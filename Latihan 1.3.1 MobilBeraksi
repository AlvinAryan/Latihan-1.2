package textbasedapplication;

public class MobilBeraksi {
    public static void main(String[] args) {
        Mobil mobilAlvin = new Mobil();
        
        mobilAlvin.warna = "Polkadot";
        mobilAlvin.tahunProduksi = 2022;
        
        System.out.println("Warna : " + mobilAlvin.warna);
        System.out.println("Tahun Produksi : " + mobilAlvin.tahunProduksi );
    }
}
